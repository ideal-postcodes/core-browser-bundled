{
  "name": "@ideal-postcodes/core-browser-bundled",
  "version": "1.3.0",
  "description": "JS bundles for api.ideal-postcodes.co.uk clients",
  "author": {
    "name": "Ideal Postcodes",
    "email": "support@ideal-postcodes.co.uk",
    "url": "https://ideal-postcodes.co.uk"
  },
  "bugs": {
    "url": "https://github.com/ideal-postcodes/core-browser-bundled/issues",
    "email": "support@ideal-postcodes.co.uk"
  },
  "homepage": "https://github.com/ideal-postcodes/core-browser-bundled",
  "repository": {
    "type": "git",
    "url": "https://github.com/ideal-postcodes/core-browser-bundled.git"
  },
  "scripts": {
    "semantic-release": "semantic-release --no-ci",
    "build:umd:ie11": "rollup --config umd.ie11.config.js",
    "build:umd": "rollup --config umd.config.js",
    "build:esm": "rollup --config esm.config.js",
    "build:esm:modern": "rollup --config esm.modern.config.js",
    "start": "http-server",
    "build": "npm run build:umd && npm run build:umd:ie11 && npm run build:esm && npm run build:esm:modern",
    "prepublishOnly": "npm run build"
  },
  "main": "dist/core-browser.umd.min.js",
  "module": "dist/core-browser.esm.modern.min.js",
  "files": [
    "dist"
  ],
  "prettier": {
    "trailingComma": "es5"
  },
  "engines": {
    "node": ">=8.0"
  },
  "keywords": [
    "API",
    "UK",
    "Address",
    "Postcode",
    "Search",
    "Ideal Postcodes",
    "Browser"
  ],
  "release": {
    "extends": "@cablanchard/semantic-release/dist/npm"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "~7.8.0",
    "@babel/preset-env": "~7.8.2",
    "@cablanchard/semantic-release": "~1.3.0",
    "babel": "~6.23.0",
    "core-js": "~3.6.0",
    "http-server": "~0.12.0",
    "prettier": "~1.19.1",
    "rollup": "~1.31.0",
    "rollup-plugin-babel": "~4.3.3",
    "rollup-plugin-commonjs": "~10.1.0",
    "rollup-plugin-node-resolve": "~5.2.0",
    "rollup-plugin-terser": "~5.2.0",
    "semantic-release": "~15.14.0",
    "@ideal-postcodes/core-browser": "~1.2.1"
  }
}
